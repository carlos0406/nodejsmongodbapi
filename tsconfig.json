{
  "compilerOptions": {
  "target": "es2017",
  "module": "commonjs",
  "noEmit": true,
  "strict": false,
  
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true,
  "baseUrl": "./src",
  "paths": {
    "@controllers/*":["controllers/*"],
    "@schemas/*":["schemas/*"]
  }
},
"include": ["src","__tests__"]
}